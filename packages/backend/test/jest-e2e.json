{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@env": "<rootDir>/src/env",
    "^@core/(.*)$": ["<rootDir>/src/core/$1"],
    "^@models/(.*)$": ["<rootDir>/src/models/$1"],
    "^@modules/(.*)$": ["<rootDir>/src/modules/$1"],
    "^@utils/(.*)$": ["<rootDir>/src/utils/$1"],
    "@entity": ["<rootDir>/src/entity/index.entity"],
    "@middleware": ["<rootDir>/src/middleware/index.middleware"],
    "^@setting/(.*)$": ["<rootDir>/src/setting/$1"]
  }
}
